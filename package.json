    "vue-social-share": "0.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "^3.11.0",
